<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="prueba.list">
      <field name="name">prueba list</field>
      <field name="model">prueba.prueba</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="value"/>
          <field name="value2"/>
          <field name="fecha"/>
          
        </tree>
      </field>
    </record>

    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="prueba.action_window">
      <field name="name">prueba window</field>
      <field name="res_model">prueba.prueba</field>
      <field name="view_mode">tree,form</field>
    </record>

    <!-- server action to the one above -->
   

    <!-- Top menu item -->
    <menuitem name="prueba" id="prueba.menu_root"/>

    <!-- menu categories -->

    <menuitem name="Menu 1" id="prueba.menu_1" parent="prueba.menu_root"/>
    <menuitem name="Menu 2" id="prueba.menu_2" parent="prueba.menu_root"/>
    <menuitem name="Menu 3" id="prueba.menu_3" parent="prueba.menu_root"/>

    <!-- actions -->

    <menuitem name="List" id="prueba.menu_1_list" parent="prueba.menu_1"
              action="prueba.action_window"/>
    <menuitem name="prueba1" id="prueba.menu_2_list" parent="prueba.menu_1"
              action="prueba.action_window"/> 
    <menuitem name="prueba2" id="prueba.menu_3_list" parent="prueba.menu_1"
              action="prueba.action_window"/>

              <menuitem name="prueba1" id="prueba.menu_4_list" parent="prueba.menu_2"
              action="prueba.action_window"/>                          
              <menuitem name="prueba2" id="prueba.menu_5_list" parent="prueba.menu_2"
              action="prueba.action_window"/>                          

              <menuitem name="prueba4" id="prueba.menu_6_list" parent="prueba.menu_3"
              action="prueba.action_window"/>                          
              <menuitem name="prueba5" id="prueba.menu_7_list" parent="prueba.menu_3"
              action="prueba.action_window"/>
              <menuitem name="prueba6" id="prueba.menu_8_list" parent="prueba.menu_3"
              action="prueba.action_window"/>                                                    
              

              

  </data>
</odoo>